{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CircuitBreaker": {
    "FailureThreshold": 3,
    "ResetTimeoutSeconds": 60
  },
  "Proxy": {
    "RequestTimeoutSeconds": 30
  },
  "Kestrel": {
    "DisableStringReuse": false,
    "AddServerHeader": false
  }
}
